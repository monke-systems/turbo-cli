{
  "name": "@monkee/turbo-cli",
  "version": "1.2.0",
  "description": "Turbo cli",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "turbo-cli": "bin/cli.js"
  },
  "files": [
    "lib",
    "bin",
    "templates"
  ],
  "scripts": {
    "start": "ts-node src/run-ui.ts",
    "build": "tsc --project tsconfig.build.json",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --passWithNoTests -t",
    "test:e2e": "jest --passWithNoTests --config tests/jest-e2e.json -t",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,tests}/**/*.ts\" --fix",
    "clean": "rm -rf lib",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monke-systems/turbo-cli"
  },
  "bugs": {
    "url": "https://github.com/monke-systems/turbo-cli/issues"
  },
  "homepage": "https://github.com/monke-systems/turbo-cli#readme",
  "dependencies": {
    "handlebars": "4.7.7",
    "inquirer": "8.2.5",
    "loglevel": "1.8.0"
  },
  "devDependencies": {
    "@monkee/eslint-config": "^1.0.0",
    "@types/inquirer": "8.2.6",
    "@types/jest": "29.5.3",
    "@types/node": "18.17.0",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint": "8.45.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
